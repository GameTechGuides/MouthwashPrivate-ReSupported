{
    "$schema": "misc\\config.schema.json",
    "versions": [
        "2021.6.30"
    ],
    "clusterName": "Capybara",
    "nodeId": 0,
    "checkForUpdates": true,
    "autoUpdate": false,
    "exitConfirmation": true,
    "defaultLanguage": "en",
    "socket": {
        "port": 22023,
        "broadcastUnknownGamedata": false,
        "messageOrdering": false,
        "ip": "auto"
    },
    "plugins": {
        "loadDirectory": true,
        "hbplugin-mouthwashgg-auth": {
            "baseUrl": "https://accounts.mouthwash.midlight.studio"
        }
    },
    "anticheat": {
        "penalty": {
            "action": "disconnect",
            "strikes": 2,
            "banAfterXDisconnects": 3,
            "banDuration": 3600,
            "disconnectMessage": "You have been banned for $duration."
        },
        "rules": {}
    },
    "logging": {
        "hideSensitiveInfo": true,
        "connections": {
            "format": [
                "id",
                "ip",
                "ping",
                "room"
            ]
        },
        "rooms": {
            "format": [
                "players",
                "map"
            ]
        },
        "players": {
            "format": [
                "id",
                "ping",
                "ishost"
            ]
        }
    },
    "reactor": true,
    "rooms": {
        "checkChatMode": false,
        "chatCommands": false,
        "plugins": {
            "loadDirectory": true
        },
        "gameCodes": "v2",
        "enforceSettings": {},
        "serverAsHost": true,
        "serverPlayer": {
            "name": "<color=yellow>[Server]</color>",
            "color": "Yellow",
            "hat": "None",
            "skin": "None"
        },
        "createTimeout": 10
    }
}